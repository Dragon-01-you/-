{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://dragon01.pythonanywhere.com/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}